{
	"name": "gaari",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --threshold error",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"scrape": "cd scripts && npx tsx scrape.ts",
		"scrape:bergenlive": "cd scripts && npx tsx scrape.ts bergenlive",
		"scrape:visitbergen": "cd scripts && npx tsx scrape.ts visitbergen",
		"scrape:bergenkommune": "cd scripts && npx tsx scrape.ts bergenkommune",
		"scrape:kulturikveld": "cd scripts && npx tsx scrape.ts kulturikveld",
		"scrape:barnasnorge": "cd scripts && npx tsx scrape.ts barnasnorge",
		"scrape:studentbergen": "cd scripts && npx tsx scrape.ts studentbergen",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"devDependencies": {
		"@eslint/js": "^10.0.1",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-vercel": "^6.3.2",
		"@sveltejs/kit": "^2.21.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"eslint": "^10.0.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.15.0",
		"globals": "^17.3.0",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.5.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.56.0",
		"vite": "^6.0.0",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"@resvg/resvg-js": "^2.6.2",
		"@supabase/supabase-js": "^2.97.0",
		"bits-ui": "^1.0.0",
		"clsx": "^2.1.0",
		"lucide-svelte": "^0.474.0",
		"resend": "^6.9.2",
		"satori": "^0.19.2",
		"sharp": "^0.34.5",
		"tailwind-merge": "^3.0.0",
		"tailwind-variants": "^1.0.0"
	},
	"type": "module"
}
